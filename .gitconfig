[user]
	name = Cameron Lane
	email = crlane@adamanteus.com
[core]
	excludesfile = /home/crlane/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
[pull]
	rebase = true
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 227
	frag = magenta bold
	commit = 227 bold
	old = red bold
	new = green bold
	whitespace = red reverse
